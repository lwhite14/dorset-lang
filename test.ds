fn unary!(v)
{
    if v then
        0
    else
        1;
}

fn unary-(v)
{
    0-v;
}

fn binary> 10 (LHS RHS)
{
    RHS < LHS;
}

fn binary| 5 (LHS RHS)
{
    if LHS then
        1
    else if RHS then
        1
    else
        0;
}

fn binary& 6 (LHS RHS)
{
    if !LHS then
        0
    else
        !!RHS;
}

fn binary= 9 (LHS RHS)
{
    !(LHS < RHS | LHS > RHS);
}


fn binary: 1 (x y)
{ 
    y;
}

fn printdensity(d)
{
    if d > 8 then
        printf(" ", 0)
    else if d > 4 then
        printf(".", 0)
    else if d > 2 then
        printf("+", 0)
    else
        printf("*", 0);
}

fn main()
{
    printdensity(1): 
    printdensity(2): 
    printdensity(3):
    printdensity(4): 
    printdensity(5): 
    printdensity(9):
    printf("%f", 10);
}