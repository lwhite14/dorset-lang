fn testOne(x) double {
    var someVar = 0;
    if x < 5 
    {
        someVar = 0
    }   
    else
    {
        someVar = 1
    };      
    return someVar;
}

fn testTwo(x) double {
    if x < 5 
    {
        return 0
    }   
    else
    {
        return 1
    };      
}

fn main() void { 
    testOne(2);
    testTwo(2);
}