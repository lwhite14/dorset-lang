fn newLine() void {
    printf("\n", 0);
}


fn main() void {
    var numbers[3] = (1, 3, 12);

    var soloNumber = numbers[2];

    printf("The third number in the first array is: %f", soloNumber);
    newLine();


    var numbersTwo[2];
    soloNumber = numbersTwo[1];

    printf("The first number in the second array is: %f", soloNumber);
    newLine();

    numbersTwo[1] = 3;
    soloNumber = numbersTwo[1];

    printf("This new number should be 3: %f", soloNumber);
    newLine();
}
