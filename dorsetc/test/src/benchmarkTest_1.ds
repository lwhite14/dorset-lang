// Calc --- Benchmark Test
// dorset-lang 0.2.0-alpha.6

fn main() double {
    var op = 3;
    var n = 24;

    var Arr[n];
    for (var i = 0, i < n, 1.0) {
        Arr[i] = op;
    }

    var result = 1;

    for (var i = 0, i < n, 1.0) {

        if (Arr[i] == 0) {
            result = result + 1;
        } 
        else {
            if (Arr[i] == 1) {
                result = result - 1;
            } 
            else {
                if (Arr[i] == 2) {
                    result = result * i;
                } 
                else {   
                    if (Arr[i] == 3) {
                        result = result + i;
                    } 
                    else {
                        if (Arr[i] == 4) {
                            result = result - i;
                        } 
                        else {
                            result = 0;
                        }
                    }   
                }
            }
        }

    }

    printf("Result is: %f", result);
    newLine();

    return result;
}