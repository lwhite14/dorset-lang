fn returnOne() double {
    return 1;
}

fn main() void {
    var numbers[3] = (1, 3, 12);


    var someNumber = numbers[returnOne()];

    printf("Expected: 3. Real: %f.", someNumber);
    newLine();


    someNumber = numbers[1 + 1];

    printf("Expected: 12. Real: %f.", someNumber);
    newLine();
}
